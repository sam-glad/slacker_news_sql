<% @articles.each do |article| %>
    <li>
      <IMG SRC="arrow.gif">
        <a href="http://<%= article["url"] %>" target="_blank" ><%= article["title"] %><small> </small></a>
          <br>
          <small><%= article["description"] %></small><br>
          <small><%= rand(1..100) %> points by someone at <%= article["created_at"] %> | </small><a href="comment.html"><small><%= rand(1..100) %> comments</small>
    </li>
<% end %>
